{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent",
          "scroll-behavior",
          "-webkit-tap-highlight-color",
          "-webkit-overflow-scrolling",
          "overflow-anchor"
        ]
      }
    ],
    "no-inline-styles": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 107",
    "not ios_saf <= 14.8"
  ]
}